{
    "general": {
        "version": "1.0.0",
        "author": "Security Research Team",
        "default_port": 8080,
        "ssl_enabled": true,
        "auto_deploy": false,
        "stealth_mode": true,
        "max_sessions": 100,
        "session_timeout": 3600
    },
    "logging": {
        "level": "INFO",
        "log_to_file": true,
        "log_to_console": true,
        "max_log_size": "10MB",
        "backup_count": 5,
        "log_format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    },
    "security": {
        "enable_rate_limiting": true,
        "max_requests_per_minute": 60,
        "block_suspicious_ips": true,
        "enable_honeypot": false,
        "capture_screenshots": true,
        "log_user_agents": true,
        "log_referrers": true
    },
    "server": {
        "bind_address": "0.0.0.0",
        "worker_processes": 4,
        "keep_alive_timeout": 30,
        "max_request_size": "16MB",
        "enable_compression": true,
        "custom_headers": {
            "Server": "nginx/1.18.0",
            "X-Powered-By": "PHP/7.4.0"
        }
    },
    "ssl": {
        "auto_generate_certs": true,
        "cert_path": "./certs/",
        "key_size": 2048,
        "cert_validity_days": 365,
        "enable_hsts": true,
        "hsts_max_age": 31536000
    },
    "templates": {
        "auto_update": false,
        "custom_css": true,
        "responsive_design": true,
        "enable_2fa_bypass": false,
        "capture_additional_data": true
    },
    "reporting": {
        "generate_reports": true,
        "report_format": "json",
        "include_screenshots": true,
        "email_notifications": false,
        "webhook_url": "",
        "export_formats": ["json", "csv", "html"]
    },
    "deployment": {
        "auto_deploy_to_domains": false,
        "ftp_settings": {
            "host": "",
            "username": "",
            "password": "",
            "port": 21,
            "passive_mode": true
        },
        "ssh_settings": {
            "host": "",
            "username": "",
            "key_path": "",
            "port": 22
        }
    },
    "advanced": {
        "enable_analytics": true,
        "track_mouse_movements": false,
        "track_keystrokes": false,
        "enable_geolocation": true,
        "browser_fingerprinting": true,
        "social_engineering_mode": "standard"
    }
}